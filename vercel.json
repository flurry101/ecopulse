{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_AUTH0_DOMAIN": "@auth0-domain",
    "NEXT_PUBLIC_AUTH0_CLIENT_ID": "@auth0-client-id",
    "AUTH0_CLIENT_SECRET": "@auth0-client-secret",
    "NEXT_PUBLIC_API_URL": "@api-url"
  }
}
